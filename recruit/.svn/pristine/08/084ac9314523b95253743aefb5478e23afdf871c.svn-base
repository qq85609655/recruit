package com.csot.recruit.model.nresume;

/**
 * 表示人类的性别
 * @author honglu
 *
 */
public enum Gender {
  /**
   * 保密
   */
  Private(0),
  
  /**
   * 男
   */
  Male(1),
  
  /**
   * 女
   */
  Female(2);
  
  private int value;
  
  private Gender(int value) {
    this.setValue(value);
  }
  
  public int getValue() {
    return value;
  }
  
  public void setValue(int value) {
    this.value = value;
  }
}
