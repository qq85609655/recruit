  <select id="selectByExample" resultMap="BaseResultMap" parameterType="com.csot.recruit.model.resume.ResumeCriteria" >
    <include refid="Oracle_Dialect_Pagination_Prefix" />
    select
    <if test="distinct" >
      distinct
    </if>
    <include refid="Base_Column_List" />
    from T_RESUME_BASICINFOR
    <if test="_parameter != null" >
      <include refid="Example_Where_Clause" />
    </if>
    <if test="orderByClause != null" >
      order by ${orderByClause}
    </if>
    <include refid="Oracle_Dialect_Pagination_Suffix" />
  </select>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select 
    <include refid="Base_Column_List" />,
    <include refid="position" />,
    <include refid="Education_Experience_List" />,
    <include refid="Work_Intention_List" />,
    <include refid="Work_Experience_List" />,
    <include refid="Language_List" />,
    <include refid="Label_List" />,
    <include refid="log_list" />
    from T_RESUME_BASICINFOR resume
    left join T_RECRUIT_POSITION position on(resume.positionid=trim(position.recruit_position_id))
    left join T_RESUME_EDUCATION_EXPERIENCE eduexperience on(resume.id=eduexperience.resume_id )
  	left join T_RESUME_WORK_EXPERIENCE workexperience on(resume.id=workexperience.resume_id)
    left join T_RESUME_WORK_INTENTION workintention on(resume.id=workintention.resume_id)
    left join T_RESUME_LANGUAGE language on(resume.id=language.resume_id)
    left join T_LABEL_RESUME label_resume on (resume.id=label_resume.resume_id)
    left join T_LABEL label on (label_resume.label_id=trim(label.id))
    left join T_SYS_LOG log on (resume.id=log.operand)
    where resume.ID = #{id,jdbcType=VARCHAR} order by resume.id,eduexperience.EDU_END_DATE desc,workexperience.JOB_END_DATE desc,log.OPERATETIME
  </select>
