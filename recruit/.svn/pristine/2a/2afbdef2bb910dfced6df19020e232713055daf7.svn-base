	color: #fff;
	cursor: pointer;
	font-weight: normal;
}
.btn {
	cursor: pointer;
	box-shadow: 1px 1px 0 #888;
	color: #fff;
	font-weight: bold;
	font-size: 14px;
}
.btn:active {
	box-shadow: none;
}

