input[type="checkbox"],  input[type="radio"], label {
	vertical-align: middle;
}
input[type="text"], input[type="password"] {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: #bababa #cdcdcd #cdcdcd;
    border-image: none;
    border-style: solid;
    border-width: 1px;
    height: 23px;
    line-height: 23px;
    padding: 0 4px;
}
input, select, textarea, label {
    color: #4d4d4d;
    font-size: 12px;
}
select {
    height: 24px;
    line-height: 24px;
}
.search_btn {
    border: medium none;
    color: #fff;
    cursor: pointer;
    height: 24px;
    line-height: 22px;
    width: 54px;
    background-color: #ea5100;
    margin-right: 15px;
}
.button-blue {
    background-color: #06c;
    border: medium none;
    color: #fff;
    cursor: pointer;
    height: 24px;
    line-height: 22px;
    padding: 0 10px;
}
.a-decoration:hover:after {
	opacity: 1;
	filter:alpha(opacity=100);
	top: 18px;
  background-color: #06c;
  visibility: visible;
} 
.a-decoration:after {
  background-color: #effaff;
  visibility: hidden;
  content: "";
  height: 1px;
  left: 0;
  opacity: 0;
	filter: alpha(opacity=0);
  position: absolute;
  top: 40px;
  transition: all 0.25s ease 0s;
  width: 100%;
}
