package com.csot.recruit.model.nresume;

import java.io.Serializable;

/**
 * 附加信息
 * @author honglu
 *
 */
public class AdditionalInformation implements Serializable {

  private static final long serialVersionUID = 1L;

  // 自我评价
  private String selfAssessment;
  
  // 工作经历详情(补充工作经历集合中遗漏的信息)
  private String workExperienceDetail;
  
  // 证书
  private Certification[] certifications;
  
  // 语言技能
  private LanguageSkill[] languageSkills;
  
  // IT技能
  private ITSkill[] itSkills;
  
  // 培训经历
  private Training[] trainings;

  public String getSelfAssessment() {
    return selfAssessment;
  }

  public void setSelfAssessment(String selfAssessment) {
    this.selfAssessment = selfAssessment;
  }

  public String getWorkExperienceDetail() {
    return workExperienceDetail;
  }

  public void setWorkExperienceDetail(String workExperienceDetail) {
    this.workExperienceDetail = workExperienceDetail;
  }

  public Certification[] getCertifications() {
    return certifications;
  }

  public void setCertifications(Certification[] certifications) {
    this.certifications = certifications;
  }

  /**
   * @return the languageSkills
   */
  public LanguageSkill[] getLanguageSkills() {
    return languageSkills;
  }

  /**
   * @param languageSkills the languageSkills to set
   */
  public void setLanguageSkills(LanguageSkill[] languageSkills) {
    this.languageSkills = languageSkills;
  }

  public ITSkill[] getItSkills() {
    return itSkills;
  }

  public void setItSkills(ITSkill[] iTSkills) {
    itSkills = iTSkills;
  }

  public Training[] getTrainings() {
    return trainings;
  }

  public void setTrainings(Training[] trainings) {
    this.trainings = trainings;
  }
}
