package com.csot.recruit.common.enums;

public enum PrivacyLevel {
  PRIVATE(0),
  
  PUBLIC(1); 
  
  private int value;
  
  private PrivacyLevel(int value) {
    this.setValue(value);
  }
  
  public int getValue() {
    return value;
  }
  
  public void setValue(int value) {
    this.value = value;
  }
}
