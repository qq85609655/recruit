html, body {
  margin: 0;
  padding: 0;
  border: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}

.errorText {
  color: red;
}
.blue_radius {
    background: #effaff none repeat scroll 0 0;
    border: 1px solid #a9ccde;
    border-radius: 5px;
}
.myresume {
    border: 1px solid #79cee3;
    padding: 15px 25px;
}
.myresume table tr td {
    padding: 3px 0;
}