var skill ={
		show:function(){
			$(".skillData").show();
			var d = "<ul>";
			for(var i=1;i<skillData.length;i++){
				d+="<li class='skill' onclick=skill.chooseChild("+i+")>"+skillData[i].name+"</li>";
			}
			d+="</ul>";
			$(".skillContent").html(d);
		},
		chooseChild:function(skillCode){
			var d = "<ul>";
        	var childs = skillData[skillCode].children;
        	for(var i=0;i<childs.length;i++){
        		d+='<li onclick=\'skill.chooseData("'+childs[i].Text+'")\'>'+childs[i].Text+'</li>';
        	}
        	d+="</ul>";
        	$(".skillChildContent").html(d);
        	$(".skill").live('click',function(){
        		var lt = $(this).offset();
				var l = lt.left;
				var t = lt.top-$("#skillOffset").offset().top;
				$(".skillChildData").css({
					left:l-90,
					top:t+20
				})
        	})
          	$(".skillChildData").show();
		},
		chooseData:function(text){
			$("#skillBtn").val(text);
			$("#skillHid").val(text);
			$(".skillData").hide();
			$(".skillChildData").hide();
			$("#skillClear").show();
		}
} 