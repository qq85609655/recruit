}

.moreOparte {
    background: #fff none repeat scroll 0 0;
    border: 1px dashed #e1e5e9;
    font-size: 14px;
    margin-top: 10px;
    padding: 20px 20px 0;
}

.moreOparte .tit_blue ,.resumeContent .tit_blue {
	clear:both;
	color: #00a0e9;
    font-size: 19px;
    font-weight: normal;
    height: 32px;
    line-height: 32px;
    position: relative;
}

#moreItems {
    border: 0 none;
    overflow: hidden;
}
#moreItems li {
    border: 1px solid #e1e5e9;
    border-radius: 5px;
    color: #00a0e9;
    float: left;
    margin: 0 10px 10px 0;
    padding: 5px;
}
#moreItems li a {
    color: #333;
    margin-left: 5px;
}
#moreItems li a:hover {
    text-decoration: none;
}
.moreOparte{
	margin-left: 80px;
	margin-right: 80px;
}

#footer {
    clear: both;
    color: #6d6d6d;
    margin-bottom: 50px;
    padding: 15px 0;
    text-align: center;
    margin-left: 80px;
	margin-right: 80px;
}
#footer #footernav {
    border-top: 1px solid #cadae7;
    padding-top: 12px;
}
#footer #footernav a {
    color: #6d6d6d;
}
.grayBg,.nobd{
	display: none;
}
.increase {
    background: transparent url("../images/sprite_repeat.jpg") no-repeat scroll -285px -203px;
    cursor: pointer;
    padding-left: 20px;
}
.edit_resume {
    font-size: 13px;
    font-weight: normal;
    line-height: 17px;
    position: relative;
    right: 30px;
    top: 9px;
    float:right;
}
a {
    color: #00a0e9;
    text-decoration: none;
}

.plus, .minus {
    cursor: pointer;
    float: left;
    height: 16px;
    margin: 4px 5px 0;
    width: 14px;
}
.plus {
    background: rgba(0, 0, 0, 0) url("../images/bigplus.jpg") no-repeat scroll 0 0;
}
.minus {
    background: rgba(0, 0, 0, 0) url("../images/minus.jpg") no-repeat scroll 0 0;
}
