	display:none;
}
#expStartDate,#expEndDate,#startYearMonth,#endYearMonth{
	position: relative;
	top:10px;
}
.red{
	color: red;
}

.workConData dl {
    border-bottom: 1px solid #e1e5e9;
    margin-bottom: 10px;
    padding-bottom: 10px;
}

.workConData dt {
 
    margin-bottom: 10px;
    font-size: 13px;
}

.workConData dd span {
    color: #999;
    display: inline-block;
    vertical-align: top;
    width: 90px;
    font-size: 12px;
    
}

.workConData .f_r{
	float: right;
	margin-right: 0px;
}

.edit {
    height: 17px;
    padding-left: 18px;
}
