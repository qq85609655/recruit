.residencyData,.pData{
	display:none;
	position:absolute;
	border: 2px solid #CCCCCC;
	background-color: white;
}

.residencyData{
	width:500px;
	height: 200px;
}

.residencyData li,.cContent dd,.pContent li,.cContent li{
	display: inline-block;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space:nowrap;
	cursor: pointer; 
	width: 100px;
	float:left;
}
.residencyTitle,.pTitle{
	position:relative;
	height: 30px;
	background-color: #E0EDFE;
}

.residencyTitle{
   width: 500px;
}

.residencyTitle span{
	position: relative;
	top:5px;
	left: 10px;
}
.closeResidency,.pClose,{
	float: right;
	margin-right: 20px;
}
.residencyContent,.pContent{
	width:500px;
	margin-left: 20px;
	margin-top: 10px;
}
.residencyChild,.cData{
	background-color: white;
	min-height: 50px;
	display: none;
	position: absolute;
	border:1px solid #CCCCCC;
	z-index: 1000;
	padding-left: 5px;
	padding-bottom: 10px;
}

.residencyChild{
    width: 560px;
}
.residencyChildTitle,.cTitle{
	width: 560px;
	height: 10px;
}

.residencyChildContent,.cContent{
	position: relative;
	width: 500px;
}

.residencyChild dd{
	width: 140px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space:nowrap;
	cursor: pointer; 
}

