}
address, caption, cite, code, dfn, em, strong, th, var {
    font-style: normal;
    font-weight: normal;
}

.basicFormTable .input_txt {
    border: 1px solid #cdcdcd;
    border-radius: 5px;
    margin: 5px 0;
    padding: 5px;
    height: 40px;
}

.w150{
	width: 175px;
}

#Email,#IDNumber{
	width: 270px;
}

#IDAddress,#Address{
	width: 700px;
}

.submit_btn, .submit_btn_pop {
    background: #11b5fa none repeat scroll 0 0;
    border: 0 none;
    color: #fff;
    cursor: pointer;
    width: 72px;
	height: 35px;
    padding: 5px 0;
    text-align: center;
}
.cancel_btn, .cancel_btn_pop {
    background: #73bffa none repeat scroll 0 0;
    border: 0 none;
    color: #fff;
    cursor: pointer;
    padding: 5px 0;
    text-align: center;
    width: 72px;
	height: 35px;
}
.submit_btn:hover, .submit_btn_pop:hover {
    background: #09acf1 none repeat scroll 0 0;
}
.cancel_btn:hover, .cancel_btn_pop:hover {
    background: #65b4f2 none repeat scroll 0 0;
}
.choose_btn {
    background: transparent url("../images/sprite_repeat.jpg") no-repeat scroll 0 -110px;
    border: medium none;
    color: #4d4d4d;
    cursor: pointer;
    height: 28px;
    padding-left: 6px;
    text-align: left;
    width: 159px;
}

.tit_blue{
	color: #B0B4E9;
	margin-left: 30px;
	font-size: 18px;
	margin-top: 15px;
}
.bd_c8e4f1{
	background-color: #F4F4F4;
	margin-top: 5px;
	padding: 20px;
}
input, select, textarea {
    color: #4d4d4d;
}
textarea {
    height: 200px;
    width: 100%;
}
.pt10{
	margin-top: 20px;
	margin-left: 20px;
}

#workCon .input_txt,#editEducationForm .input_txt{
    border: 1px solid #cdcdcd;
