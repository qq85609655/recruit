package org.amberframework.web.miniui.pojo;

import java.io.Serializable;
import java.util.List;

/***
 * 页面返回对象
 *
 * @param <T>
 */
public class MiniRtn2Grid<T> implements Serializable {

	private static final long serialVersionUID = -1316140508570811804L;

	// 返回页面对象集合
	private List<T> data;
	// 总数量
	private int total;

	public List<T> getData() {
		return data;
	}

	public void setData(List<T> data) {
		this.data = data;
	}

	public int getTotal() {
		return total;
	}

	public void setTotal(int total) {
		this.total = total;
	}
}
