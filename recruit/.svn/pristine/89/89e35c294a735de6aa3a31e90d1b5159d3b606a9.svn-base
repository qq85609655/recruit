package com.csot.recruit.model.nresume;

import java.io.Serializable;
import java.util.Date;

/**
 * 表示标准化后的简历中的个人信息部分
 * @author honglu
 *
 */
public class PersonalInformation implements Serializable {
  
  private static final long serialVersionUID = 1L;

  /** 联系地址 */
  private String address;
  
  /** 备选电话 */
  private String alternativePhone;
  
  /** 出生日期 */
  private Date dateOfBirth;
  
  /** 居住地 */
  private PropertyData residency;
  
  /** 证件类型 */
  private PropertyData idType;
  
  /** 证件号码 */
  private String idNumber;
  
  /** 户口 */
  private PropertyData huKou;
  
  /** 婚姻状况 */
  private PropertyData maritalStatus;
  
  /** 国籍 */
  private PropertyData nationality;
  
  /** 电话 */
  private String phone;
  
  /** 邮政编码 */
  private String postcode;
  
  /** 性别 */
  private PropertyData gender;
  
  /** 姓名 */
  private String name;
  
  /** 邮件地址 */
  private String email;
  
  /** 工作年限 */
  private Integer yearsOfWorkExperience;

  public String getAddress() {
    return address;
  }

  public void setAddress(String address) {
    this.address = address;
  }

  public String getAlternativePhone() {
    return alternativePhone;
  }

  public void setAlternativePhone(String alternativePhone) {
    this.alternativePhone = alternativePhone;
  }

  public Date getDateOfBirth() {
    return dateOfBirth;
  }

  public void setDateOfBirth(Date dateOfBirth) {
    this.dateOfBirth = dateOfBirth;
  }

  public PropertyData getIdType() {
    return idType;
  }

  public void setIdType(PropertyData idType) {
    this.idType = idType;
  }

  /**
   * @return the idNumber
   */
  public String getIdNumber() {
    return idNumber;
  }

  /**
   * @param idNumber the idNumber to set
   */
  public void setIdNumber(String idNumber) {
    this.idNumber = idNumber;
  }

  public PropertyData getHuKou() {
    return huKou;
  }

  public void setHuKou(PropertyData huKou) {
    this.huKou = huKou;
  }

  /**
   * @return the maritalStatus
   */
  public PropertyData getMaritalStatus() {
    return maritalStatus;
  }

  /**
   * @param maritalStatus the maritalStatus to set
   */
  public void setMaritalStatus(PropertyData maritalStatus) {
    this.maritalStatus = maritalStatus;
  }

  /**
   * @return the nationality
   */
  public PropertyData getNationality() {
    return nationality;
  }

  /**
   * @param nationality the nationality to set
   */
  public void setNationality(PropertyData nationality) {
    this.nationality = nationality;
  }

  public String getPhone() {
    return phone;
  }

  public void setPhone(String phone) {
    this.phone = phone;
  }

  public String getPostcode() {
    return postcode;
  }

  public void setPostcode(String postcode) {
    this.postcode = postcode;
  }

  /**
   * @return the gender
   */
  public PropertyData getGender() {
    return gender;
  }

  /**
   * @param gender the gender to set
   */
  public void setGender(PropertyData gender) {
    this.gender = gender;
  }

  public String getName() {
    return name;
  }

  public void setName(String name) {
    this.name = name;
  }

  public String getEmail() {
    return email;
  }

  public void setEmail(String email) {
    this.email = email;
  }

  public Integer getYearsOfWorkExperience() {
    return yearsOfWorkExperience;
  }

  public void setYearsOfWorkExperience(Integer yearsOfWorkExperience) {
    this.yearsOfWorkExperience = yearsOfWorkExperience;
  }

  public PropertyData getResidency() {
    return residency;
  }

  public void setResidency(PropertyData residency) {
    this.residency = residency;
  }
}
