package com.csot.recruit.common.enums;

public enum PositionStatus {
  
  DRAFT(0,"草稿"),  // 草稿
  
  COMPLETED(1,"已完成"), // 已完成
  
  PUBLISHING(2,"发布中"), // 发布中 
  
  EXPIRED(3,"已过期 "), // 已过期 
  
  EXPIRING(4,"即将到期"), // 即将到期
  
  PAUSED(5,"已暂停"); // 已暂停
  
  public int tag; 
  
  public String msg;
  
  private PositionStatus(int tag, String msg) {
   this.tag = tag;
   this.msg = msg;
  }
  
  public int getTag() {
    return this.tag;    
  }
  
  public String getMsg() {
    return this.msg;    
  }
}
