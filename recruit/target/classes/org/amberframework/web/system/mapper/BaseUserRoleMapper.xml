<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.amberframework.web.system.dao.BaseUserRoleMapper" >
    <select id="getRoleIdByUserId" parameterType="java.lang.String" resultType="java.lang.String">
      select role_id from t_sys_user_role where user_id = #{userId}
    </select>
</mapper>