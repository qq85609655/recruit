<?xml version="1.0" encoding="UTF-8"?>
<excelExport>
  <!-- 
       可配置元素：
  showIndex  是否显示行序号  boolean类型，值为true或false；
  indexColumnNum  行序号所在列号  如果showIndex为true，必须配置。值为数字，从0开始计算；
  excelColumn  导出Excel的列配置，目前支持以下属性：
          name  导出dateset对象的属性名称；
          cellIndex  导出在Excel中的列位置，列号从0开始计算；
          type  导出值类型，各类型说明请参考ExcelExportParameter的常量值；
          sourceDateFormat（用于日期时间格式导出）  导出dateset对象属性的源日期格式；
          exportDateFormat（用于日期时间格式导出）  导出后需写入Excel中的目标日期格式
   -->
  <showIndex>true</showIndex>
  <indexColumnNum>0</indexColumnNum>
  <excelColumn name="name" cellIndex="1" type="0"></excelColumn>
  <excelColumn name="gender" cellIndex="2" type="0"></excelColumn>
  <excelColumn name="height" cellIndex="3" type="0"></excelColumn>
  <excelColumn name="salary" cellIndex="4" type="0"></excelColumn>
  <excelColumn name="birthday" cellIndex="5" type="0" sourceDateFormat="yyyy/MM/dd HH:mm:ss"
    exportDateFormat="yyyy-MM-dd HH:mm:ss"></excelColumn>
  <excelColumn name="entryDate" cellIndex="6" type="0" sourceDateFormat="yyyy-MM-dd HH:mm:ss"
    exportDateFormat="yyyy/MM/dd HH:mm:ss"></excelColumn>
  <excelColumn constantValue="my comments" cellIndex="7" type="1"></excelColumn>
  <excelColumn constantValue="my constants" cellIndex="8" type="1"></excelColumn>
</excelExport>